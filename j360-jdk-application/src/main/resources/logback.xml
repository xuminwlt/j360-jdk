<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<jmxConfigurator />

	<appender name="console" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%date{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
		</encoder>
	</appender>

	<logger name="me.j360">
		<level value="DEBUG" />
	</logger>

	<!-- root -->
	<root level="DEBUG">
		<appender-ref ref="console" />
	</root>
</configuration>